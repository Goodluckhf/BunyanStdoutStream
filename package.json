{
  "name": "bunyan-stdout-stream",
  "version": "1.0.0",
  "description": "stdout pretty print, human readable stream for bunyan",
  "main": "dist/index.js",
  "scripts": {
    "test": "mocha --ui bdd --reporter spec --require babel-register ./test/*.test.js ./test/**/*.test.js",
    "test:coverage:dev": "nyc --reporter=text --require babel-register mocha --ui bdd --reporter spec --require babel-register ./test/*.test.js ./test/**/*.test.js",
    "test:coverage": "nyc --reporter=lcov --require babel-register mocha --ui bdd --reporter spec --require babel-register ./test/*.test.js ./test/**/*.test.js",
    "report-coverage": "cat coverage/lcov.info | coveralls",
    "prebuild": "rimraf ./dist",
    "build": "babel src -d ./dist -s --ignore test.js",
    "precommit": "lint-staged"
  },
  "engines": {
    "node": ">=6.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Goodluckhf/BunyanStdoutStream.git"
  },
  "keywords": [
    "bunyan",
    "stdout",
    "logs",
    "log",
    "stream",
    "bunyan stream",
    "stdout logs",
    "debug",
    "pretty logs"
  ],
  "author": "Goodluckhf <Goodluckhf@yandex.ru>",
  "license": "MIT",
  "dependencies": {
    "cli-color": "^1.2.0",
    "deep-extend": "^0.6.0",
    "moment": "^2.22.2",
    "nyc": "^12.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.3",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bunyan": "^1.8.12",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-flowtype": "^2.49.3",
    "eslint-plugin-import": "^2.12.0",
    "husky": "^0.14.3",
    "lint-staged": "^7.1.3",
    "lodash": "^4.17.10",
    "mocha": "^5.2.0",
    "mocha-lcov-reporter": "^1.3.0"
  },
  "bugs": {
    "url": "https://github.com/Goodluckhf/BunyanStdoutStream/issues"
  },
  "homepage": "https://github.com/Goodluckhf/BunyanStdoutStream#readme",
  "lint-staged": {
    "*.js": "eslint"
  },
  "eslintIgnore": [
    "flow-typed/*"
  ],
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint-config-airbnb-base",
      "plugin:flowtype/recommended"
    ],
    "parser": "babel-eslint",
    "plugins": [
      "flowtype"
    ],
    "rules": {
      "no-multi-spaces": 0,
      "indent": [
        "error",
        "tab"
      ],
      "no-tabs": 0,
      "key-spacing": [
        "error",
        {
          "align": {
            "beforeColon": false,
            "afterColon": true,
            "on": "colon"
          }
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allowAfterThis": true
        }
      ],
      "spaced-comment": 0,
      "no-trailing-spaces": 0,
      "arrow-body-style": [
        1,
        "as-needed"
      ],
      "flowtype/space-before-type-colon": 0,
      "flowtype/space-after-type-colon": 0
    }
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "current"
          },
          "useBuiltIns": "usage",
          "loose": true
        }
      ]
    ],
    "plugins": [
      [
        "transform-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  }
}
