{
  "name": "bunyan-stdout-stream",
  "version": "1.0.0",
  "description": "stdout pretty print, human readable stream for bunyan",
  "main": "index.js",
  "scripts": {
    "test": "mocha --ui bdd --reporter spec --require @babel/register ./test/*.test.js ./test/**/*.test.js",
    "test:coverage:dev": "nyc --reporter=text --require @babel/register mocha --ui bdd --reporter spec --require @babel/register ./test/*.test.js ./test/**/*.test.js",
    "test:coverage": "nyc --reporter=lcov --require @babel/register mocha --ui bdd --reporter spec --require @babel/register ./test/*.test.js ./test/**/*.test.js",
    "lint": "eslint --ignore-path .gitignore \"**/*.js\"",
    "report-coverage": "cat coverage/lcov.info | coveralls",
    "prebuild": "rimraf ./dist",
    "build": "babel src -d ./dist -s --ignore test.js",
    "precommit": "lint-staged",
    "semantic-release": "cp package.json ./dist && cp LICENSE ./dist && cp -r docs ./dist && cp README.md ./dist && semantic-release"
  },
  "engines": {
    "node": ">=6.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Goodluckhf/BunyanStdoutStream.git"
  },
  "keywords": [
    "bunyan",
    "stdout",
    "logs",
    "log",
    "stream",
    "bunyan stream",
    "stdout logs",
    "debug",
    "pretty logs"
  ],
  "author": "Goodluckhf <Goodluckhf@yandex.ru>",
  "license": "MIT",
  "dependencies": {
    "bson": "4.0.2",
    "cli-color": "1.4.0",
    "deep-extend": "0.6.0",
    "moment": "2.24.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "bunyan": "^1.8.12",
    "chai": "^4.1.2",
    "coveralls": "^3.0.1",
    "eslint": "^6.0.1",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-flowtype": "^3.11.1",
    "eslint-plugin-import": "^2.12.0",
    "husky": "^3.0.9",
    "lint-staged": "^9.1.0",
    "lodash": "^4.17.14",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1",
    "semantic-release": "^15.13.19"
  },
  "bugs": {
    "url": "https://github.com/Goodluckhf/BunyanStdoutStream/issues"
  },
  "homepage": "https://github.com/Goodluckhf/BunyanStdoutStream#readme",
  "lint-staged": {
    "*.js": "eslint --fix --ignore-path .gitignore"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "eslint-config-airbnb-base"
    ],
    "parser": "babel-eslint",
    "rules": {
      "no-multi-spaces": 0,
      "indent": [
        "error",
        "tab"
      ],
      "no-tabs": 0,
      "key-spacing": [
        "error",
        {
          "align": {
            "beforeColon": false,
            "afterColon": true,
            "on": "colon"
          }
        }
      ],
      "no-underscore-dangle": [
        "error",
        {
          "allowAfterThis": true
        }
      ],
      "spaced-comment": 0,
      "no-trailing-spaces": 0,
      "arrow-body-style": [
        1,
        "as-needed"
      ],
      "flowtype/space-before-type-colon": 0,
      "flowtype/space-after-type-colon": 0
    }
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          },
          "loose": true
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-object-rest-spread",
        {
          "useBuiltIns": true
        }
      ]
    ]
  },
  "release": {
    "analyzeCommits": {
      "preset": "angular",
      "releaseRules": [
        {
          "type": "docs",
          "release": "patch"
        },
        {
          "type": "refactor",
          "release": "patch"
        },
        {
          "type": "chore",
          "release": "patch"
        },
        {
          "type": "style",
          "release": "patch"
        }
      ]
    },
    "pkgRoot": "dist"
  }
}
